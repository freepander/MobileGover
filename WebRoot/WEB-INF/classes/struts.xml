<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE struts PUBLIC	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"	"http://struts.apache.org/dtds/struts-2.1.dtd"><struts>	<constant name="struts.devMode" value="true"></constant>	<constant name="struts.multipart.maxSize" value="500000000"/>	<package name="curl" namespace="/curl" extends="struts-default">		<action name="*_*" class="{1}Action" method="{2}">			<result name="list" type="chain">{1}_list</result>			<result name="success">/main/{1}_{2}.jsp</result>			<result name="itemList">/main/pictureNews_itemList.jsp</result>			<result name="departmentList">/main/department_list.jsp</result>		</action>	</package>	<package name="index" namespace="/app" extends="struts-default">		<action name="news_*" class="newsAction" method="{1}">			<result name="newsList">/newsList.jsp</result>			<result name="newsContent">/newsContent.jsp</result>		</action>		<action name="app_*" class="appAction" method="{1}">			<result name="videoContent">/app/video.jsp</result>		</action>	</package></struts>	